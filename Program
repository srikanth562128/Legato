package com.java;

public class Test {
	public int square(int a)
	{
		return a*a;
	}
	public int cube(int a)
	{
		return a*a*a;
	}
	public String  prime(int a)
	{
		if(a%2==0)
		{
			return a+"is not a prime";
		}
		for(int i=0;i<=a/2;i++)
		{
			if(a%i==0)
			{
				return a+"is not a Prime";
			}
			
		}
		return a+"is a Prime";
		}

}
